<div class="home">
    <div class="income-data">
        <h3>INCOME DATAs</h3>
        <table class="table table-success table-striped">
            <tr>
                <td>ID</td>
                <td>Source</td>
                <td>Categories</td>
                <td>Description</td>
                <td>Amount</td>
            </tr>
        {{user}}
       
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.Name}}</td>
            <td>{{this.Username}}</td>
            <td>{{this.Password}}</td>
            <td>{{this.Amount}}</td>
        </tr>
        
        {{!-- {{/if}} --}}

        <tr>
            <th>TOTAL INCOME:</th>
            <td></td>
            <td></td>
            <td></td>
            <td>{{total.income}}</td>
        </tr>
        </table>
    </div>
    

  

